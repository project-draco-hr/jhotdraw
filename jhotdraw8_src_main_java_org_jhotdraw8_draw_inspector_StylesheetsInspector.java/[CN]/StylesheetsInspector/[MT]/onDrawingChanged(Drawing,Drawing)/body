{
  if (oldValue != null) {
    listView.getItems().clear();
  }
  if (newValue != null) {
    List<URI> stylesheets=newValue.get(Drawing.AUTHOR_STYLESHEETS);
    if (stylesheets == null) {
      listView.getItems().clear();
    }
 else {
      listView.getItems().setAll(stylesheets);
    }
  }
  counter=0;
}
