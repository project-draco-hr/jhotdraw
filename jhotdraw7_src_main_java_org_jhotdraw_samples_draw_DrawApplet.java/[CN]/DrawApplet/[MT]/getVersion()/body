{
  if (version == null) {
    BufferedReader r=null;
    try {
      r=new BufferedReader(new InputStreamReader(getClass().getResourceAsStream("version.txt"),"UTF-8"));
      version=r.readLine();
    }
 catch (    Throwable e) {
      version="unknown";
    }
 finally {
      if (r != null)       try {
        r.close();
      }
 catch (      IOException e) {
      }
    }
  }
  return version;
}
