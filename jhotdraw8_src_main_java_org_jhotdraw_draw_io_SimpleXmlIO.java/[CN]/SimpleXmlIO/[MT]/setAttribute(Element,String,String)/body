{
  if (namespaceURI == null || namespaceQualifier == null) {
    if (!elem.hasAttribute(unqualifiedName))     elem.setAttribute(unqualifiedName,value);
  }
 else {
    if (!elem.hasAttributeNS(namespaceURI,namespaceQualifier + ":" + unqualifiedName))     elem.setAttributeNS(namespaceURI,namespaceQualifier + ":" + unqualifiedName,value);
  }
}
