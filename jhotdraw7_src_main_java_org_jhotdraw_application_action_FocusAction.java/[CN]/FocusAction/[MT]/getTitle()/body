{
  ResourceBundleUtil labels=ResourceBundleUtil.getLAFBundle("org.jhotdraw.application.Labels");
  String title=labels.getString("unnamedFile");
  if (project != null) {
    File file=project.getFile();
    if (file == null) {
      title=labels.getString("unnamedFile");
    }
 else {
      title=file.getName();
    }
    if (project.hasUnsavedChanges()) {
      title+="*";
    }
    title=(labels.getFormatted("internalFrameTitle",title,project.getWindowManager().getName(),project.getMultipleOpenId()));
  }
  return title;
}
