{
  OutputFormat.super.write(file,drawing);
  if (exportSlices) {
    List<Figure> slices=new ArrayList<>();
    for (    Figure f : drawing.preorderIterable()) {
      if (sliceClass.isAssignableFrom(f.getClass())) {
        slices.add(f);
      }
    }
    writeSlices(file.getParentFile(),drawing,slices);
  }
}
