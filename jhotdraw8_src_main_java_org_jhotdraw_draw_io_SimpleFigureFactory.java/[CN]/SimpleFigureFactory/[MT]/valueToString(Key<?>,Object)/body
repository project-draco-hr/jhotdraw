{
  @SuppressWarnings("unchecked") Converter<Object> converter=(Converter<Object>)valueToXML.get(key.getFullValueType());
  if (converter == null) {
    throw new IOException("no converter for attribute type " + key.getFullValueType());
  }
  @SuppressWarnings("unchecked") String string=converter.toString(value);
  return string;
}
