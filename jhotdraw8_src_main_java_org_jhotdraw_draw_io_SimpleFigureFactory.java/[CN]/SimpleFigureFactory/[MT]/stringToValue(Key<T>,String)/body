{
  try {
    @SuppressWarnings("unchecked") Converter<T> converter=(Converter<T>)valueFromXML.get(key.getFullValueType());
    if (converter == null) {
      throw new IOException("no converter for key \"" + key + "\" with attribute type "+ key.getFullValueType());
    }
    return converter.fromString(CharBuffer.wrap(string),this);
  }
 catch (  ParseException ex) {
    throw new IOException(ex);
  }
}
