{
  willChange();
  if (getStartConnector() != null) {
    Point2D.Double start=getStartConnector().findStart(this);
    if (start != null) {
      basicSetStartPoint(start);
    }
  }
  if (getEndConnector() != null) {
    Point2D.Double end=getEndConnector().findEnd(this);
    if (end != null) {
      basicSetEndPoint(end);
    }
  }
  changed();
}
