{
  final Connector oldStart=startConnector;
  if (newStart != oldStart) {
    willChange();
    basicSetStartConnector(newStart);
    fireUndoableEditHappened(new AbstractUndoableEdit(){
      public String getPresentationName(){
        return ResourceBundleUtil.getLAFBundle("org.jhotdraw.draw.Labels").getString("ConnectionFigure.undoConnect");
      }
      public void undo() throws CannotUndoException {
        super.undo();
        willChange();
        basicSetStartConnector(oldStart);
        changed();
      }
      public void redo() throws CannotUndoException {
        super.redo();
        willChange();
        basicSetStartConnector(newStart);
        changed();
      }
    }
);
    changed();
  }
}
