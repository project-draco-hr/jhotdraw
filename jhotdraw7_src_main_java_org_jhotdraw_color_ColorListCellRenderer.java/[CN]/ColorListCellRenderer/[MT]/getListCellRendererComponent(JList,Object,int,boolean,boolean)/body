{
  super.getListCellRendererComponent(list,value,index,isSelected,cellHasFocus);
  if (value instanceof Color) {
    Color c=(Color)value;
    icon.setColor(c);
    setToolTipText(ColorSpaceUtil.toToolTipText(c));
    setText("");
  }
 else {
    icon.setColor(null);
    setText("");
  }
  setIcon(icon);
  return this;
}
