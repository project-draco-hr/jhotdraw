{
  String id=getId(object);
  if (id == null) {
    if (object instanceof Styleable) {
      Styleable f=(Styleable)object;
      id=f.getId();
      if (id != null && getObject(id) == null) {
        putId(object,id);
      }
 else {
        id=super.createId(object,f.getTypeSelector().toLowerCase());
      }
    }
 else {
      id=super.createId(object);
    }
  }
  return id;
}
