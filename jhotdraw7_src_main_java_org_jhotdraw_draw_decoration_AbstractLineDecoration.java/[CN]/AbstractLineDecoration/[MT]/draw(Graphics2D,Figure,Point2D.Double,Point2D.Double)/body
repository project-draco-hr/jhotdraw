{
  GeneralPath path=getTransformedDecoratorPath(f,p1,p2);
  Color color;
  if (isFilled) {
    if (isSolid) {
      color=f.get(STROKE_COLOR);
    }
 else {
      color=f.get(FILL_COLOR);
    }
    if (color != null) {
      g.setColor(color);
      g.fill(path);
    }
  }
  if (isStroked) {
    color=f.get(STROKE_COLOR);
    if (color != null) {
      g.setColor(color);
      g.setStroke(AttributeKeys.getStroke(f));
      g.draw(path);
    }
  }
}
