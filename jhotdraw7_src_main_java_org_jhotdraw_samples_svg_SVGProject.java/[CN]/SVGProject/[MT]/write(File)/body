{
  OutputStream out=null;
  try {
    out=new BufferedOutputStream(new FileOutputStream(f));
    new SVGOutputFormat().write(f.toURL(),out,view.getDrawing(),view.getDrawing().getFigures());
  }
  finally {
    if (out != null)     try {
      out.close();
    }
 catch (    IOException e) {
    }
    ;
  }
}
