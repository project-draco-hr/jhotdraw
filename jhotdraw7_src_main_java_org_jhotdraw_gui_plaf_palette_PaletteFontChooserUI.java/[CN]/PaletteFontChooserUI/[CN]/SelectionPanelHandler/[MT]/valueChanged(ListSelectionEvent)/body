{
  if (isUpdating == 0) {
    Object src=evt.getSource();
    if (src == selectionPanel.getCollectionList()) {
      doCollectionChanged();
    }
 else     if (src == selectionPanel.getFamilyList()) {
      doFamilyChanged();
    }
 else     if (src == selectionPanel.getFaceList()) {
      doFaceChanged();
    }
  }
}
