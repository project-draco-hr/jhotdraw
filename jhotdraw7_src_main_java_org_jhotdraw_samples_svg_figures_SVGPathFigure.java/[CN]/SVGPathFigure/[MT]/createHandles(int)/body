{
  LinkedList<Handle> handles=new LinkedList<Handle>();
switch (detailLevel % 2) {
case 0:
    for (    Figure child : getChildren()) {
      handles.addAll(child.createHandles(detailLevel));
    }
  break;
case 1:
TransformHandleKit.addTransformHandles(this,handles);
break;
default :
break;
}
return handles;
}
