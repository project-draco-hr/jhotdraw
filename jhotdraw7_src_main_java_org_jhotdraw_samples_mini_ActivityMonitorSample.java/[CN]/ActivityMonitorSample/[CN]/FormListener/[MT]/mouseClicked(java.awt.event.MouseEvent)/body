{
  if (evt.getSource() == indicatorAll) {
    ActivityMonitorSample.this.indicatorClicked(evt);
  }
 else   if (evt.getSource() == indicatorA) {
    ActivityMonitorSample.this.indicatorClicked(evt);
  }
 else   if (evt.getSource() == indicatorB) {
    ActivityMonitorSample.this.indicatorClicked(evt);
  }
 else   if (evt.getSource() == indicatorC) {
    ActivityMonitorSample.this.indicatorClicked(evt);
  }
}
