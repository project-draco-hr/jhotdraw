{
  name=unescape(name);
  Class enumClass;
  try {
    enumClass=Class.forName(name);
  }
 catch (  ClassNotFoundException ex) {
    throw new IllegalArgumentException("Enum name not known to factory:" + name);
  }
  if (enumClass == null) {
    throw new IllegalArgumentException("Enum name not known to factory:" + name);
  }
  return Enum.valueOf(enumClass,value);
}
