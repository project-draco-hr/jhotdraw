{
  if (view() == null) {
    return;
  }
  DrawApplication window=createApplication();
  window.open();
  window.newWindow(view().drawing());
  if (view().drawing().getTitle() != null) {
    window.setDrawingTitle(view().drawing().getTitle() + " (View)");
  }
 else {
    window.setDrawingTitle(getDefaultDrawingTitle() + " (View)");
  }
}
