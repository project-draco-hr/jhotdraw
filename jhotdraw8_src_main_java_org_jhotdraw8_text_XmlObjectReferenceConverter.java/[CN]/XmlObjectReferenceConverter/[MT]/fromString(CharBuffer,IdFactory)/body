{
  String str=buf.toString();
  if ("none".equals(str)) {
    return null;
  }
  Object obj=idFactory.getObject(str);
  System.out.println("XmlObjectReferenceConverter getObject:" + str + " = "+ obj);
  @SuppressWarnings("unchecked") T value=clazz.isInstance(obj) ? (T)obj : null;
  return value;
}
