{
  StyleableMap<String,String> map=new SimpleStyleableMap<>();
  map.put("1","user1");
  map.put("2","user2");
  map.put("3","user3");
  map.put("4","user4");
  map.put(StyleOrigin.AUTHOR,"2","author2");
  map.put(StyleOrigin.AUTHOR,"3","author3");
  for (  Map.Entry<String,String> e : map.entrySet()) {
    System.out.println(e);
  }
  System.out.println("---");
}
