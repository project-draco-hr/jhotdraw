{
  ApplicationModel mo=getModel();
  if (mo.getAction(AbstractPreferencesAction.ID) == null) {
    return null;
  }
  JMenu m;
  JMenuItem mi;
  m=new JMenu();
  labels.configureMenu(m,"edit");
  if (mo.getAction(AbstractPreferencesAction.ID) != null) {
    mi=m.add(mo.getAction(AbstractPreferencesAction.ID));
    mi.setIcon(null);
  }
  return m;
}
