{
  super(source);
  if (parent == null) {
    throw new NullPointerException("parent is null");
  }
  eventType=wasAdded ? EventType.FIGURE_ADDED : EventType.FIGURE_REMOVED;
  this.figure=child;
  this.key=null;
  this.oldValue=null;
  this.newValue=null;
  this.parent=parent;
  this.index=index;
}
