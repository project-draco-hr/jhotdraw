{
  String className=cls.getName();
  StringBuffer sb=new StringBuffer();
  int i=className.lastIndexOf('.');
  if (i > 0) {
    sb.append(className.substring(0,i));
    sb.append(".resources.");
    sb.append(cls.getSimpleName());
  }
 else {
    sb.append("resources.");
    sb.append(cls.getSimpleName());
  }
  return sb.toString();
}
