{
  JMenuBar mb=new JMenuBar();
  mb.add(createFileMenu(p));
  JMenu helpMenu=null;
  String helpMenuText=labels.getString("help.text");
  for (  JMenu mm : getModel().createMenus(this,p)) {
    if (mm.getText().equals(helpMenuText)) {
      helpMenu=mm;
    }
 else {
      mb.add(mm);
    }
  }
  mb.add(createWindowMenu(p));
  if (helpMenu != null) {
    mb.add(helpMenu);
  }
  return mb;
}
