{
  if (tt.nextToken() != '{') {
    throw new ParseException("CurlyBlock: '{' expected in line " + tt.getLineNumber() + ".",tt.getPosition());
  }
  while (tt.nextToken() != CssTokenizer.TT_EOF && tt.currentToken() != '}') {
    tt.pushBack();
    parseComponentValue(tt);
  }
  if (tt.currentToken() != '}') {
    throw new ParseException("CurlyBlock: '}' expected in line " + tt.getLineNumber() + ".",tt.getPosition());
  }
  return null;
}
