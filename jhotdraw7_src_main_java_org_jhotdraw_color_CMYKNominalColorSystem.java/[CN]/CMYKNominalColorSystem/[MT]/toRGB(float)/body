{
  float cyan, magenta, yellow, black;
  cyan=component[0];
  magenta=component[1];
  yellow=component[2];
  black=component[3];
  float red, green, blue;
  red=1f - cyan * (1f - black) - black;
  green=1f - magenta * (1f - black) - black;
  blue=1f - yellow * (1f - black) - black;
  return 0xff000000 | ((int)(red * 255) << 16) | ((int)(green * 255) << 8)| (int)(blue * 255);
}
