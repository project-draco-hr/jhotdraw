{
  StringBuilder b=new StringBuilder();
  for (int i=0, n=g.getVertexCount(); i < n; i++) {
    T v=g.getVertex(i);
    if (g.getNextCount(v) == 0) {
      b.append(v).append('\n');
    }
 else {
      for (int j=0, m=g.getNextCount(v); j < m; j++) {
        b.append(v).append(" -> ").append(g.getNext(v,j)).append('\n');
      }
    }
  }
  return b.toString();
}
