{
  float h=hsb[0];
  h=h - (float)Math.floor(h);
  if (h < 0) {
    h=1 + h;
  }
  if (hMin < 0) {
    return (1f + hMin <= h || h <= hMax) && sMin <= hsb[1] && hsb[1] <= sMax && bMin <= hsb[2] && hsb[2] <= bMax;
  }
 else {
    return hMin <= h && h <= hMax && sMin <= hsb[1] && hsb[1] <= sMax && bMin <= hsb[2] && hsb[2] <= bMax;
  }
}
