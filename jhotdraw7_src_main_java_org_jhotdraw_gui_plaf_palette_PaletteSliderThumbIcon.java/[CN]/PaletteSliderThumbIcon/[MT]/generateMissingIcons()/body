{
  Icon[] oldIcons;
  if (icons.length != 6) {
    oldIcons=new Icon[6];
    System.arraycopy(icons,0,oldIcons,0,Math.min(icons.length,6));
  }
 else {
    oldIcons=icons;
  }
  if (icons[EP] == null) {
    icons[EP]=icons[E];
  }
  if (icons[D] == null) {
    icons[D]=icons[E];
  }
  if (icons[I] == null) {
    icons[I]=icons[E];
  }
  if (icons[DI] == null) {
    icons[DI]=icons[D];
  }
}
