{
  if (version == null) {
    BufferedReader r=null;
    try {
      InputStream resource=AbstractSVGDrawingApplet.class.getResourceAsStream("version.txt");
      r=new BufferedReader(new InputStreamReader(resource,"UTF-8"));
      version=r.readLine();
    }
 catch (    IOException e) {
      version="unknown";
    }
catch (    NullPointerException e) {
      version="unknown";
    }
 finally {
      if (r != null) {
        try {
          r.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
  return version;
}
