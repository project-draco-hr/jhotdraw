{
  Comparable min=getMinimum();
  try {
    if (min != null && min.compareTo(value) > 0) {
      return false;
    }
  }
 catch (  ClassCastException cce) {
    if (wantsCCE) {
      throw cce;
    }
    return false;
  }
  Comparable max=getMaximum();
  try {
    if (max != null && max.compareTo(value) < 0) {
      return false;
    }
  }
 catch (  ClassCastException cce) {
    if (wantsCCE) {
      throw cce;
    }
    return false;
  }
  return true;
}
