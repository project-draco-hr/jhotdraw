{
  if (isUpdatingSlider++ == 0) {
    if (getView() == null || attributeKey == null) {
      setValue(0);
    }
 else {
      Double sliderValue=null;
      boolean isFirst=true;
      Figure f=getView().getDrawing();
      sliderValue=attributeKey.get(f);
      if (sliderValue != null) {
        setValue((int)(sliderValue * scaleFactor));
      }
    }
    repaint();
  }
  isUpdatingSlider--;
}
