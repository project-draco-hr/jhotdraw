{
  if (cm == cachedModel) {
    if (cached != null) {
      WritableRaster ras=(WritableRaster)cached.get();
      if (ras != null && ras.getWidth() >= w && ras.getHeight() >= h) {
        cached=null;
        return ras;
      }
    }
  }
  if (w < 32)   w=32;
  if (h < 32)   h=32;
  return cm.createCompatibleWritableRaster(w,h);
}
