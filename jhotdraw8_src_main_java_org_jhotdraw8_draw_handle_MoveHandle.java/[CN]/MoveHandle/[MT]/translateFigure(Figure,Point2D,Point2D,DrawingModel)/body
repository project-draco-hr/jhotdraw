{
  Point2D npl=f.worldToLocal(newPoint);
  Point2D opl=f.worldToLocal(oldPoint);
  Transform tx=Transform.translate(npl.getX() - opl.getX(),npl.getY() - opl.getY());
  System.out.println("MoveHandle.translate: " + tx);
  if (model != null) {
    model.reshape(f,tx);
  }
 else {
    f.reshape(tx);
  }
}
