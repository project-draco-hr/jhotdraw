{
  try {
    Drawing d=(Drawing)t.getTransferData(dataFlavor);
    if (replace) {
      for (      Map.Entry<AttributeKey,Object> e : d.getAttributes().entrySet()) {
        drawing.set(e.getKey(),e.getValue());
      }
    }
    for (    Figure f : d.getChildren()) {
      drawing.add(f);
    }
  }
 catch (  Throwable th) {
    th.printStackTrace();
  }
}
