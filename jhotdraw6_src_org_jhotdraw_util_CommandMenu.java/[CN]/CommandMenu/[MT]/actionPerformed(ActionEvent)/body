{
  int j=0;
  Object source=e.getSource();
  for (int i=0; i < getItemCount(); i++) {
    JMenuItem item=getItem(i);
    if (getMenuComponent(i) instanceof JSeparator) {
      continue;
    }
    if (source == item) {
      Command cmd=(Command)fCommands.elementAt(j);
      cmd.execute();
      break;
    }
    j++;
  }
}
