{
  if (value == null || value.isEmpty()) {
    out.append("none");
    return;
  }
  boolean first=true;
  for (  Point2D p : value) {
    if (first) {
      first=false;
    }
 else {
      out.append(", ");
    }
    out.append(doubleConverter.toString(p.getX()));
    out.append(' ');
    out.append(doubleConverter.toString(p.getY()));
  }
}
