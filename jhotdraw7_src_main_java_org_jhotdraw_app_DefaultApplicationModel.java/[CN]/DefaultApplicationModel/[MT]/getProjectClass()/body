{
  if (projectClass == null) {
    if (projectClassName != null) {
      try {
        projectClass=Class.forName(projectClassName);
      }
 catch (      Exception e) {
        InternalError error=new InternalError("unable to get project class");
        error.initCause(e);
        throw error;
      }
    }
  }
  return projectClass;
}
