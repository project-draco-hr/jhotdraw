{
  Rectangle2D.Double bounds=owner.getBounds();
  if ((owner instanceof DecoratedFigure) && ((DecoratedFigure)owner).getDecorator() != null) {
    Insets2D.Double insets=DECORATOR_INSETS.get(owner);
    if (insets != null) {
      insets.addTo(bounds);
    }
  }
  return new Point2D.Double(bounds.x + bounds.width * relativeX,bounds.y + bounds.height * relativeY);
}
