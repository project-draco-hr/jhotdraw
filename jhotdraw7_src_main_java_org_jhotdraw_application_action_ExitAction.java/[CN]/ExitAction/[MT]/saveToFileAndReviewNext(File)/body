{
  unsavedProject.execute(new Worker(){
    public Object construct(){
      try {
        unsavedProject.write(file);
        return null;
      }
 catch (      IOException e) {
        return e;
      }
    }
    public void finished(    Object value){
      fileSavedAndReviewNext(unsavedProject,file,value);
    }
  }
);
}
