{
  if (value == null) {
    if (nullable) {
      out.append("none");
      return;
    }
 else {
      throw new IllegalArgumentException("value is null");
    }
  }
  out.append('/');
  stringConverter.toString(out,value.getFind());
  out.append('/');
  if (!"$1".equals(value.getReplace())) {
    stringConverter.toString(out,value.getReplace());
    out.append('/');
  }
}
