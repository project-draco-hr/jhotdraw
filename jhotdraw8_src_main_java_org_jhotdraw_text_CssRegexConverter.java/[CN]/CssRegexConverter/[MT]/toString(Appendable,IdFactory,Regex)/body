{
  if (value == null) {
    if (nullable) {
      out.append("none");
      return;
    }
 else {
      throw new IllegalArgumentException("value is null");
    }
  }
  String find=value.getFind();
  if (find == null) {
    out.append("none");
    return;
  }
  stringConverter.toString(out,find);
  String replace=value.getReplace();
  if (replace != null) {
    out.append(' ');
    stringConverter.toString(out,replace);
  }
}
