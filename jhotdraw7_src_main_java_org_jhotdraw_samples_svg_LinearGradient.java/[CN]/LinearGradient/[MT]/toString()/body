{
  StringBuilder buf=new StringBuilder();
  buf.append("LinearGradient@");
  buf.append(hashCode());
  buf.append('(');
  for (int i=0; i < stopOffsets.length; i++) {
    if (i != 0)     buf.append(',');
    buf.append(stopOffsets[i]);
    buf.append('=');
    buf.append(stopColors[i]);
  }
  buf.append(')');
  return buf.toString();
}
