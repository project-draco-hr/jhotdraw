{
  try {
    LinearGradient that=(LinearGradient)super.clone();
    that.stopOffsets=this.stopOffsets.clone();
    that.stopColors=this.stopColors.clone();
    return that;
  }
 catch (  CloneNotSupportedException ex) {
    InternalError e=new InternalError();
    e.initCause(ex);
    throw e;
  }
}
