{
  buf.append('A').append(Double.toString(radiusX)).append(',').append(Double.toString(radiusY)).append(' ').append(Double.toString(xAxisRotation)).append(',').append(largeArcFlag ? '1' : '0').append(' ').append(sweepFlag ? '1' : '0').append(' ').append(Double.toString(x)).append(',').append(Double.toString(y));
}
