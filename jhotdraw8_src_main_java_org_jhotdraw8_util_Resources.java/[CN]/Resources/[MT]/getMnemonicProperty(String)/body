{
  String s;
  try {
    s=getStringRecursive(key + ".mnemonic");
  }
 catch (  MissingResourceException e) {
    if (isVerbose) {
      System.err.println("Warning Resources[" + baseName + "] \""+ key+ ".mnemonic\" not found.");
    }
    s=null;
  }
  return (s == null || s.length() == 0) ? null : KeyCombination.valueOf(s);
}
