{
  Object obj=null;
  try {
    obj=resource.getObject(key);
  }
 catch (  MissingResourceException e) {
    if (parent != null) {
      return parent.handleGetObjectRecursively(key);
    }
 else {
      obj="";
      System.err.println("Can't find resource for bundle " + baseName + ", key "+ key);
    }
  }
  return obj;
}
