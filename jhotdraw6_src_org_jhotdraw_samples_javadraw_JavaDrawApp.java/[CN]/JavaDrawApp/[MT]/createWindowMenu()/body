{
  CommandMenu menu=new CommandMenu("Window");
  Command cmd=new AbstractCommand("New View",this){
    public void execute(){
      newView();
    }
  }
;
  menu.add(cmd);
  cmd=new AbstractCommand("New Window",this,false){
    public void execute(){
      newWindow(createDrawing());
    }
  }
;
  menu.add(cmd);
  return menu;
}
