{
  JMenu menu=new JMenu("Window");
  JMenuItem mi=new JMenuItem("New View");
  mi.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      newView();
    }
  }
);
  menu.add(mi);
  mi=new JMenuItem("New Window");
  mi.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      newWindow();
    }
  }
);
  menu.add(mi);
  return menu;
}
