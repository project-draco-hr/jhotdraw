{
  if (name == null) {
    throw new InvalidParameterException("setParameter(" + name + ","+ value+ ") name must not be null");
  }
  if (value == null) {
    throw new InvalidParameterException("setParameter(" + name + ","+ value+ ") value must not be null");
  }
  boundary();
  writeName(name);
  newline();
  newline();
  writeln(value);
}
