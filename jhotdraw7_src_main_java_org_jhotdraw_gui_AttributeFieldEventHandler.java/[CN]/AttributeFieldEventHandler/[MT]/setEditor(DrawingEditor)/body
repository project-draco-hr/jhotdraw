{
  if (editor != null) {
    editor.removePropertyChangeListener(this);
    if (getCurrentView() != null) {
      getCurrentView().removeFigureSelectionListener(this);
    }
  }
  editor=newValue;
  if (editor != null) {
    editor.addPropertyChangeListener(this);
    if (getCurrentView() != null) {
      getCurrentView().addFigureSelectionListener(this);
    }
  }
}
