{
  if (updatingDepth++ == 0) {
    if (format != null) {
      try {
        double newValue=format.parse(getText()).doubleValue();
        if (newValue >= minimum && newValue <= maximum) {
          setValue(newValue);
        }
      }
 catch (      ParseException ex) {
      }
    }
  }
  updatingDepth--;
}
