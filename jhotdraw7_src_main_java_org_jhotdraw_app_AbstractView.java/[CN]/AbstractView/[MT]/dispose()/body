{
  if (executor != null) {
    executor.shutdown();
    executor=null;
  }
  if (disposables != null) {
    for (    Disposable d : disposables) {
      d.dispose();
    }
    disposables=null;
  }
  if (openChooser != null) {
    openChooser=null;
  }
  if (saveChooser != null) {
    saveChooser=null;
  }
  removeAll();
}
