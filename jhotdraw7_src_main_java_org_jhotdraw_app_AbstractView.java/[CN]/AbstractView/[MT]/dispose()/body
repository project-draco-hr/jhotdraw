{
  if (executor != null) {
    executor.shutdown();
    executor=null;
  }
  if (disposables != null) {
    for (    Disposable d : (LinkedList<Disposable>)disposables.clone()) {
      d.dispose();
    }
    disposables=null;
  }
  if (openChooser != null) {
    openChooser=null;
  }
  if (saveChooser != null) {
    saveChooser=null;
  }
  removeAll();
}
