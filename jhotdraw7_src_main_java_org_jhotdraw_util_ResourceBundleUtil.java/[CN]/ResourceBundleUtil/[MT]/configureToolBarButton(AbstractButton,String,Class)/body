{
  Icon icon=getIconProperty(argument,baseClass);
  if (icon != null) {
    button.setIcon(getIconProperty(argument,baseClass));
    button.setText(null);
  }
 else {
    button.setIcon(null);
    button.setText(getTextProperty(argument));
  }
  button.setToolTipText(getToolTipTextProperty(argument));
}
