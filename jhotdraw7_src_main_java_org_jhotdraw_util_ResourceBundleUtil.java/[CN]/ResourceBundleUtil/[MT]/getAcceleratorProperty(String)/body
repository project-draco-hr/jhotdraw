{
  KeyStroke ks=null;
  try {
    String s;
    s=resource.getString(key + ".accelerator");
    if (s == null) {
      s=resource.getString(key + ".acc");
    }
    ks=(s == null) ? (KeyStroke)null : KeyStroke.getKeyStroke(s);
  }
 catch (  MissingResourceException e) {
  }
catch (  NoSuchElementException e) {
  }
  return ks;
}
