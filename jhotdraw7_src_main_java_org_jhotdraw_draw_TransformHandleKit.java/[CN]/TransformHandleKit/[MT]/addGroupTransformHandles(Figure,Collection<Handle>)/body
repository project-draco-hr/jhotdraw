{
  handles.add(new BoundsOutlineHandle(f,GROUP_BOUNDS_STROKE_1,GROUP_BOUNDS_COLOR_1,GROUP_BOUNDS_STROKE_2,GROUP_BOUNDS_COLOR_2,GROUP_BOUNDS_STROKE_1_DISABLED,GROUP_BOUNDS_COLOR_1_DISABLED,GROUP_BOUNDS_STROKE_2_DISABLED,GROUP_BOUNDS_COLOR_2_DISABLED));
  addCornerTransformHandles(f,handles);
  addEdgeTransformHandles(f,handles);
  handles.add(new RotateHandle(f));
}
