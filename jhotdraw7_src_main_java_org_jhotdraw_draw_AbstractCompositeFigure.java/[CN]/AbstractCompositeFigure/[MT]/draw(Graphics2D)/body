{
  Rectangle2D clipBounds=g.getClipBounds();
  if (clipBounds != null) {
    for (    Figure child : children) {
      if (child.isVisible() && child.getDrawingArea().intersects(clipBounds)) {
        child.draw(g);
      }
    }
  }
 else {
    for (    Figure child : children) {
      if (child.isVisible()) {
        child.draw(g);
      }
    }
  }
  if (isConnectorsVisible()) {
    drawConnectors(g);
  }
}
