{
  Vector selected=view().selectionZOrdered();
  Drawing drawing=view().drawing();
  if (selected.size() > 0) {
    view().clearSelection();
    drawing.orphanAll(selected);
    GroupFigure group=new GroupFigure();
    group.addAll(selected);
    view().addToSelection(drawing.add(group));
  }
  view().checkDamage();
}
