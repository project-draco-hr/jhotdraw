{
  if (DEBUG)   System.out.println("UndoRedoManager.add " + anEdit);
  if (undoOrRedoInProgress) {
    anEdit.die();
    return true;
  }
  boolean success=super.addEdit(anEdit);
  updateActions();
  if (success && anEdit.isSignificant() && editToBeUndone() == anEdit) {
    setHasSignificantEdits(true);
  }
  return success;
}
