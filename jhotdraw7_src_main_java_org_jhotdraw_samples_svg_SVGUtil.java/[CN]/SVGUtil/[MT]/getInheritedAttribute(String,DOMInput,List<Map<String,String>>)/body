{
  List<String> values=in.getInheritedAttribute(name);
  for (int i=values.size() - 1; i >= 0; i--) {
    if (values.get(i) != null) {
      return values.get(i);
    }
    if (styles.get(i) != null && styles.get(i).containsKey(name)) {
      return styles.get(i).get(name);
    }
  }
  return null;
}
