{
  JMenuItem mi;
  JMenu m;
  m=new JMenu();
  labels.configureMenu(m,(model.getAction(LoadFileAction.ID) != null || model.getAction(LoadDirectoryAction.ID) != null) ? "file.loadRecent" : "file.openRecent");
  m.setIcon(null);
  m.add(model.getAction(ClearRecentFilesMenuAction.ID));
  OpenRecentMenuHandler handler=new OpenRecentMenuHandler(m,view);
  return m;
}
