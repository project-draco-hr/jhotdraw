{
  ObservableList<Node> list=p.getChildrenUnmodifiable();
  for (int i=list.size() - 1; i >= 0; i--) {
    Node n=list.get(i);
    Point2D pl=n.parentToLocal(pp);
    if (n.contains(pl)) {
      Figure f=nodeToFigureMap.get(n);
      if (f == null) {
        if (n instanceof Parent) {
          f=findFigure((Parent)n,pl);
        }
      }
      return f;
    }
  }
  return null;
}
