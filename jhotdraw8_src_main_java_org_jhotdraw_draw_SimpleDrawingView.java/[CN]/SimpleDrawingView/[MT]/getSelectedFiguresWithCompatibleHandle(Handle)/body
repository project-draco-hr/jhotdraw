{
  validateHandles();
  ArrayList<Figure> figures=new ArrayList<>();
  for (  Map.Entry<Figure,List<Handle>> entry : handles.entrySet()) {
    for (    Handle h : entry.getValue()) {
      if (h.isCompatible(master)) {
        figures.add(entry.getKey());
        break;
      }
    }
  }
  return figures;
}
