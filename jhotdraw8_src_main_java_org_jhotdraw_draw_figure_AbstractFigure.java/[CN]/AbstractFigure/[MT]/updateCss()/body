{
  getStyleableMap().clearNonUserValues();
  Drawing d=getDrawing();
  if (d != null) {
    StylesheetsManager<Figure> styleManager=d.getStyleManager();
    styleManager.applyStylesheetsTo(this);
    for (    Figure child : getChildren()) {
      child.updateCss();
    }
  }
  invalidateTransforms();
}
