{
  int count=0;
  ArrayList list=current.getChildren();
  for (int i=0; i < list.size(); i++) {
    XMLElement node=(XMLElement)list.get(i);
    if (node.getName().equals(tagName)) {
      if (count++ == index) {
        stack.push(current);
        current=node;
        return;
      }
    }
  }
  throw new IOException("no such element:" + tagName + " at index:"+ index);
}
