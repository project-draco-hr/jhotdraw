{
  Polynomial result=new Polynomial();
  double d1=this.getDegree();
  double d2=that.getDegree();
  double dmax=max(d1,d2);
  for (int i=0; i <= dmax; i++) {
    double v1=(i <= d1) ? this.coefs[i] : 0;
    double v2=(i <= d2) ? that.coefs[i] : 0;
    result.coefs[i]=v1 + v2;
  }
  return result;
}
