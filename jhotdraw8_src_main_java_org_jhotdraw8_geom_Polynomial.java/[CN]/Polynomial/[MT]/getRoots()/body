{
  double[] result;
  this.simplify();
switch (this.getDegree()) {
case 0:
    result=new double[0];
  break;
case 1:
result=this.getLinearRoot();
break;
case 2:
result=this.getQuadraticRoots();
break;
case 3:
result=this.getCubicRoots();
break;
case 4:
result=this.getQuarticRoots();
break;
default :
result=new double[0];
}
return result;
}
