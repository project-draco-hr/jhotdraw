{
  if (oldValue != null) {
    uninstallProjectListeners(oldValue);
  }
  if (newValue != null) {
    installProjectListeners(newValue);
  }
  firePropertyChange("documentView",oldValue,newValue);
  updateEnabled(oldValue != null && oldValue.isEnabled(),newValue != null && newValue.isEnabled());
  updateProperty();
}
