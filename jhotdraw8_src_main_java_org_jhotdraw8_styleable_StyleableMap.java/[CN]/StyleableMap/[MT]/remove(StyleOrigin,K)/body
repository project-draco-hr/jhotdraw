{
  StyledValue sv=backingMap.get(key);
  V ret=null;
  boolean hadValue;
  if (sv != null && sv.hasValue(o)) {
    ret=sv.getValue(o);
    SimpleChange change=new SimpleChange(key,ret,null,false,true);
    callObservers(o,true,change);
    sv.removeValue(o);
    callObservers(o,false,change);
  }
 else {
    ret=null;
  }
  return ret;
}
