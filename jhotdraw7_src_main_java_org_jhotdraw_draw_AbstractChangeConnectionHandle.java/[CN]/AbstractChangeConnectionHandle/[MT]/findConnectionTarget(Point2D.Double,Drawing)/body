{
  Figure targetFigure=findConnectableFigure(p,drawing);
  if (getSource() == null && targetFigure != null) {
    return findConnector(p,targetFigure,getConnection());
  }
 else {
    if ((targetFigure != null) && targetFigure.canConnect() && targetFigure != originalTarget && !targetFigure.includes(getOwner()) && canConnect(getSource().getOwner(),targetFigure)) {
      return findConnector(p,targetFigure,getConnection());
    }
  }
  return null;
}
