{
  if (key == SVGAttributeKeys.TRANSFORM || key == SVGAttributeKeys.FONT_FACE || key == SVGAttributeKeys.FONT_BOLD || key == SVGAttributeKeys.FONT_ITALIC || key == SVGAttributeKeys.FONT_SIZE || key == SVGAttributeKeys.STROKE_WIDTH || key == SVGAttributeKeys.STROKE_COLOR || key == SVGAttributeKeys.STROKE_GRADIENT) {
    invalidate();
  }
  super.setAttribute(key,newValue);
}
