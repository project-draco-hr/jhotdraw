{
  getNode().getScene().getStylesheets().addAll(GrapherApplication.class.getResource("/org/jhotdraw/draw/gui/inspector.css").toString(),GrapherApplication.class.getResource("/org/jhotdraw/samples/grapher/grapher.css").toString());
  Preferences prefs=Preferences.userNodeForPackage(GrapherProjectView.class);
  PreferencesUtil.installVisibilityPrefsHandlers(prefs,detailsScrollPane,detailsVisible,mainSplitPane,Side.RIGHT);
}
