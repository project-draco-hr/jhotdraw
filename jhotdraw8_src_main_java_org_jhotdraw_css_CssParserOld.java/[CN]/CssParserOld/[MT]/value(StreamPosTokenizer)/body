{
switch (tt.ttype) {
case StreamPosTokenizer.TT_EOF:
    return "end of file";
case StreamPosTokenizer.TT_EOL:
  return "end of line";
case StreamPosTokenizer.TT_NUMBER:
return Double.toString(tt.nval);
case StreamPosTokenizer.TT_WORD:
return "\"" + tt.sval + "\"";
default :
return "'" + (char)tt.ttype + "'";
}
}
