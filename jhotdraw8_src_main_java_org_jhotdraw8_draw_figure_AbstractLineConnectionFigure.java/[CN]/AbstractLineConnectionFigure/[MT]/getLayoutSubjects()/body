{
  final Figure startTarget=get(START_TARGET);
  final Figure endTarget=get(END_TARGET);
  if (startTarget == null && endTarget == null) {
    return Collections.emptySet();
  }
  Set<Figure> ctf=new HashSet<>();
  if (startTarget != null) {
    ctf.add(startTarget);
  }
  if (endTarget != null) {
    ctf.add(endTarget);
  }
  return ctf;
}
