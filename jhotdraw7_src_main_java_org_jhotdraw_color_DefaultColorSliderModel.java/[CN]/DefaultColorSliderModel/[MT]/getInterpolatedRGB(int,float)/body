{
  float[] c=new float[system.getComponentCount()];
  int j=0;
  for (  DefaultBoundedRangeModel brm : componentModels) {
    c[j]=((brm.getValue() - brm.getMinimum()) / (float)(brm.getMaximum() - brm.getMinimum())) * (system.getMaxValue(j) - system.getMinValue(j)) + system.getMinValue(j);
    j++;
  }
  c[i]=componentValue;
  return system.toRGB(c);
}
