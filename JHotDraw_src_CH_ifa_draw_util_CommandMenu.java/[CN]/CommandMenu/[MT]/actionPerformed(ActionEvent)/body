{
  int j=0;
  Object source=e.getSource();
  for (int i=0; i < getItemCount(); i++) {
    MenuItem item=getItem(i);
    if (item.getLabel().equals("-"))     continue;
    if (source == item) {
      Command cmd=(Command)fCommands.elementAt(j);
      cmd.execute();
      break;
    }
    j++;
  }
}
