{
  Project p=getActiveProject();
  if (p != null) {
    try {
      return p.getClass().getMethod(getterName,(Class[])null).invoke(p);
    }
 catch (    Throwable e) {
      InternalError error=new InternalError("No " + getterName + " method on "+ p);
      error.initCause(e);
      throw error;
    }
  }
  return null;
}
