{
  float radius=(float)Math.min(w,h);
  for (int index=0; index < pixels.length; index++) {
    if (alphas[index] != 0) {
      pixels[index]=alphas[index] | 0xffffff & ColorSpaceUtil.toRGB(colorSpace,angulars[index],radials[index],brights[index]);
    }
  }
  newPixels();
  isPixelsValid=false;
}
