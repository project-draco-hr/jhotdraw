{
  ActionMap am=app.getActionMap(v);
  Action a;
  if (null != (a=am.get(CutAction.ID))) {
    add(m,a);
  }
  if (null != (a=am.get(CopyAction.ID))) {
    add(m,a);
  }
  if (null != (a=am.get(PasteAction.ID))) {
    add(m,a);
  }
  if (null != (a=am.get(DuplicateAction.ID))) {
    add(m,a);
  }
  if (null != (a=am.get(DeleteAction.ID))) {
    add(m,a);
  }
}
