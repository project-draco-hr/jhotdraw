{
  if (oldValue != null) {
    uninstallViewListeners(oldValue);
  }
  if (newValue != null) {
    installViewListeners(newValue);
  }
  firePropertyChange(VIEW_PROPERTY,oldValue,newValue);
  updateEnabled(oldValue != null && oldValue.isEnabled(),newValue != null && newValue.isEnabled());
  updateView();
}
