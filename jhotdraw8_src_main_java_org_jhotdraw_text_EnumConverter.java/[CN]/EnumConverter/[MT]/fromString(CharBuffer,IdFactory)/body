{
  int pos=in.position();
  StringBuilder out=new StringBuilder();
  while (in.remaining() > 0 && !Character.isWhitespace(in.charAt(0))) {
    out.append(in.get());
  }
  if (out.length() == 0) {
    in.position(pos);
    throw new ParseException("word expected",pos);
  }
  return Enum.valueOf(enumClass,out.toString());
}
