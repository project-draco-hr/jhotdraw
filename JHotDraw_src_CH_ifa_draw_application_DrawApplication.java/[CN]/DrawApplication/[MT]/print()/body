{
  tool().deactivate();
  PrintJob printJob=getToolkit().getPrintJob(this,"Print Drawing",null);
  if (printJob != null) {
    Graphics pg=printJob.getGraphics();
    if (pg != null) {
      ((StandardDrawingView)getDesktop().getActiveDrawingView()).printAll(pg);
      pg.dispose();
    }
    printJob.end();
  }
  tool().activate();
}
