{
  DrawingView dv=getDesktop().getActiveDrawingView();
  if (dv != null && dv.isInteractive()) {
    try {
      String name=storeFormat.store(file,dv.drawing());
      dv.drawing().setTitle(name);
      updateApplicationTitle();
    }
 catch (    IOException e) {
      showStatus(e.toString());
    }
  }
}
