    ex.printStackTrace();
  }
  chooserPanel.add(createSliderChooser(ColorSpace.getInstance(ColorSpace.CS_sRGB)));
  chooserPanel.add(createSliderChooser(CMYKGenericColorSpace.getInstance()));
